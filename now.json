{
  "version": 2,
  "builds": [
    { "src": "backoffice/package.json", "use": "@now/next" },
    { "src": "website/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/backoffice/(.*)", "dest": "/backoffice/$1" },
    { "src": "/backoffice", "status": 308, "headers": { "Location": "/backoffice/" } },
    { "src": "/(.*)", "dest": "/website/$1" }
  ]
}
